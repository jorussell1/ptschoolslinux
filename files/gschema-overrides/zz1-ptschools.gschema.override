[org.gnome.desktop.background]
picture-uri='file:///usr/share/backgrounds/ptschoolslinux/sense_of_place.png'
picture-options='scaled'

[org.gnome.shell]
favorite-apps=['com.google.Chrome.desktop','org.gnome.Nautilus.desktop','io.github.kolunmi.Bazaar.desktop', 'org.gnome.Calendar.desktop','org.libreoffice.LibreOffice.desktop']

[org.gnome.desktop.wm.preferences]
