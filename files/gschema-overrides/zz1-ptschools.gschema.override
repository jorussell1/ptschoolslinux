[org.gnome.desktop.a11y]
always-show-universal-access-status=true

[org.gnome.shell]
enabled-extensions=['apps-menu@gnome-shell-extensions.gcampax.github.com','places-menu@gnome-shell-extensions.gcampax.github.com','dash-to-panel@jderose9.github.com','workspace-indicator@gnome-shell-extensions.gcampax.github.com','no-overview@fthx','ding@rastersoft.com']

